match qos-group 2
exit

class-map type network-qos Silver_Traffic_NQ
match qos-group 4
exit

Policy-map type network-qos Setup_QOS
class type network-qos Platinum_Traffic_NQ
set cos 5
mtu 9000
exit

class type network-qos Silver_Traffic_NQ
set cos 4
mtu 9000
exit

