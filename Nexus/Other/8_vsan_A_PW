config t

vlan <<var_global_fcoe_A_vlan_id>>
name FCoE_Fabric_A
fcoe vsan <<var_global_vsan_A_id>>
exit
interface Po11
switchport trunk allowed vlan add <<var_global_fcoe_A_vlan_id>>, <<var_global_fcoe_B_vlan_id>>
exit
interface vfc11
bind interface Po11
no shutdown
exit
interface Po12
switchport trunk allowed vlan add <<var_global_fcoe_A_vlan_id>>, <<var_global_fcoe_B_vlan_id>>
exit
interface vfc12
bind interface Po12
no shutdown
exit
interface san-port-channel 1
channel mode active
exit
vsan database
vsan <<var_global_vsan_A_id>> name Fabric_A
vsan <<var_global_vsan_A_id>> interface fc1/29-30
vsan <<var_global_vsan_A_id>> interface san-port-channel 1
vsan <<var_global_vsan_A_id>> interface vfc11
vsan <<var_global_vsan_A_id>> interface vfc12
exit

interface fc1/29-30
channel-group 1
no shutdown
exit

interface san-port-channel 1
channel mode active
switchport mode F
switchport trunk mode auto
exit

copy run start
